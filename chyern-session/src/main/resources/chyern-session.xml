<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="sessionManagement" class="com.github.chyern.session.processor.SessionManagement"/>
    <bean id="sessionInterceptor" class="com.github.chyern.session.interceptor.SessionInterceptor"/>
    <bean id="httpSessionStrategy" class="com.github.chyern.session.strategy.SessionStrategy"/>
    <bean id="sessionAdapter" class="com.github.chyern.session.adapter.SessionAdapter"/>
    <bean id="session" class="com.github.chyern.session.model.Session"/>
    <bean id="sessionRepository" class="com.github.chyern.session.strategy.SessionRepository"/>


</beans>